<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ie.teamchile.smartapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    
  

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ie.teamchile.smartapp.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ie.teamchile.smartapp.QuickMenuActivity"
            android:label="@string/title_activity_quick_menu"
            android:screenOrientation="portrait" />
        <activity
            android:name="ie.teamchile.smartapp.AppointmentTypeSpinnerActivity"
            android:label="@string/title_activity_appointment_type_spinner"
            android:screenOrientation="portrait" />
        <activity
            android:name="ie.teamchile.smartapp.CalendarActivity"
            android:label="@string/title_activity_calendar"
            android:screenOrientation="portrait" />
        <activity
            android:name="ie.teamchile.smartapp.SelectTimeActivity"
            android:label="@string/title_activity_select_time"
            android:screenOrientation="portrait" />
        <activity
            android:name="ie.teamchile.smartapp.HttpClientActivity"
            android:label="@string/title_activity_http_client"
            android:screenOrientation="portrait" />
        <activity
            android:name="ie.teamchile.smartapp.ServiceUserActivity"
            android:label="@string/title_activity_service_user"
            android:screenOrientation="portrait" />
        <activity
            android:name="ie.teamchile.smartapp.ServiceUserSearchActivity"
            android:label="@string/title_activity_service_user_search"
            android:screenOrientation="portrait" />
        <activity
            android:name="ie.teamchile.smartapp.ConfirmAppointmentActivity"
            android:label="@string/title_activity_confirm_appointment"
            android:screenOrientation="portrait" />
        <activity
            android:name="ie.teamchile.smartapp.AppointmentCalendarActivity"
            android:label="@string/title_activity_appointment_calendar"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="ie.teamchile.smartapp.ContactDetailsActivity"
            android:label="@string/title_activity_contact_details"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>