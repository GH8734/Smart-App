<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team3.smartapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuickMenuActivity"
            android:label="@string/title_activity_quick_menu" />
        <activity
            android:name=".AppointmentTypeSpinnerActivity"
            android:label="@string/title_activity_appointment_type_spinner" />
        <activity
            android:name=".CalendarActivity"
            android:label="@string/title_activity_calendar" />
        <activity
            android:name=".SelectTimeActivity"
            android:label="@string/title_activity_select_time" />
        <activity
            android:name=".HttpClientActivity"
            android:label="@string/title_activity_http_client" />
        <activity
            android:name=".ServiceUserActivity"
            android:label="@string/title_activity_service_user" />
        <activity
            android:name=".ServiceUserSearchActivity"
            android:label="@string/title_activity_service_user_search" />
    </application>
</manifest>